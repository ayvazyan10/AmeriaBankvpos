{
    "name": "ayvazyan10/ameriabankvpos",
    "description": "AmeriaBank VPOS service for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Razmik Ayvazyan",
            "email": "ayvazyan403@gmail.com",
            "homepage": "https://ayvazyan.pro"
        }
    ],
    "homepage": "https://github.com/ayvazyan10/ameriabankvpos",
    "keywords": [
        "Laravel",
        "AmeriaBank",
        "VPOS",
        "Payments"
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^8.0|^9.0|^10.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Ayvazyan10\\AmeriaBankVPOS\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ayvazyan10\\AmeriaBankVPOS\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ayvazyan10\\AmeriaBankVPOS\\AmeriaBankVPOSServiceProvider"
            ],
            "aliases": {
                "AmeriaBankVPOS": "Ayvazyan10\\AmeriaBankVPOS\\Facades\\AmeriaBankVPOS"
            }
        }
    }
}
